    <script src="~/assets/admin/js/glightbox.min.js"></script>
    <script src="~/assets/admin/js/isotope.pkgd.min.js"></script>
    <script src="~/assets/admin/js/swiper-bundle.min.js"></script>
    <script src="~/assets/admin/js/noframework.waypoints.js"></script>
    <script src="~/assets/admin/js/validate.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <!-- Template Main JS File -->
    <script src="~/assets/admin/js/main.js"></script>

    @RenderSection("scripts",required:false)
