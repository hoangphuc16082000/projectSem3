    <link href="~/assets/admin/css/bootstrap-icons.css" rel="stylesheet">
    <link href="~/assets/admin/css/boxicons.min.css" rel="stylesheet">
    <link href="~/assets/admin/css/glightbox.min.css" rel="stylesheet">
